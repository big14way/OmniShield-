# Private Key for deployment (never commit the actual .env file)
PRIVATE_KEY=your_private_key_here

# Etherscan API Key for contract verification
ETHERSCAN_API_KEY=your_etherscan_api_key

# Polygonscan API Key for Polygon Amoy verification
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# Alchemy API Key for Ethereum Sepolia
ALCHEMY_API_KEY=your_alchemy_api_key

# Pyth Network Configuration
PYTH_TESTNET_ENDPOINT=https://hermes.pyth.network

# Hedera Configuration
HEDERA_TESTNET_RPC=https://testnet.hashio.io/api
HEDERA_ACCOUNT_ID=0.0.xxxxx

# Gas Reporter Configuration (optional)
REPORT_GAS=false
COINMARKETCAP_API_KEY=your_coinmarketcap_api_key
